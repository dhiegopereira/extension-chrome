{
    "manifest_version": 3,
    "name": "Hide chat",
    "version": "1.0",
    "description": "Extens√£o para ocultar o chat do google meet.",
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["content_script.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "128.png"
      }
    },
    "permissions": ["activeTab"]
  }
  